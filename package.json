{
  "name": "immutable-table",
  "version": "0.0.1",
  "description": "Immutable.js powered Table data type",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:lint",
    "test:unit": "mocha --compilers js:babel-register ./test/**/*-spec.js",
    "test:watch": "watch 'npm run test' test/",
    "test:lint": "npm run lint",
    "prepublish": "npm run build",
    "build": "babel src --out-dir lib",
    "pre:build": "npm run clean",
    "clean": "rimraf build lib",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "precoverage": "nyc npm run test:unit",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "babel": {
    "presets": [ "es2015" ]
  },
  "author": "Jonathan Frederic",
  "license": "MIT",
  "dependencies": {
    "inheritable-map": "^1.0.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "immutable": "^3.8.1",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "rimraf": "^2.5.2",
    "watch": "^0.18.0"
  }
}
